{
    "name": "barryvdh/laravel-elfinder",
    "keywords": ["laravel", "elfinder", "tinymce", "filebrowser"],
    "description": "A Laravel Package to integrate elFinder 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nao-pon/elFinder-nightly",
                "version": "2.1.0.0",
                "dist": {
                    "url": "https://github.com/nao-pon/elFinder-nightly/raw/85a9ec2c417d2f25a3866dd123860bdc1927f772/latests/elfinder-2.1.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["php/"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.x|5.0.x",
        "nao-pon/elFinder-nightly": "2.1.x"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\Elfinder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev"
}
